#include <stdio.h>
#include <ctype.h>  

int main() {
    char ch;
    
    
    scanf("%c", &ch);
    
    
    if (isalpha(ch)) {
        if (isupper(ch)) 
        {
            printf("Upper\n");
        }
        else if (islower(ch))
        {
            printf("Lower\n");
        }
    } 
   
    else if (isdigit(ch)) 
    {
        printf("Number\n");
    } 
  
    else {
        printf("Symbol\n");
    }
    
    return 0;
}
